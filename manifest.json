{
    "manifest_version": 2,
    "name": "Flashcard Extension",
    "version": "1.0",
    "description": "A simple flashcard extension for students!",
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon":"icon.png"
    },
    "icons":{
        "128":"icon.png"
    },
    "permissions": [
      "activeTab"
    ]
  }
  